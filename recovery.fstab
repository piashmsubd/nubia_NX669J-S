# Recovery FSTAB for Nubia NX669J-S
/dev/block/bootdevice/by-name/system   /system    ext4   ro,barrier=1 wait
/dev/block/bootdevice/by-name/vendor   /vendor    ext4   ro,barrier=1 wait
/dev/block/bootdevice/by-name/product  /product   ext4   ro,barrier=1 wait
/dev/block/bootdevice/by-name/metadata /metadata  ext4   defaults wait
/dev/block/bootdevice/by-name/super    /super     ext4   defaults wait
/dev/block/bootdevice/by-name/userdata /data      f2fs   rw,nosuid,nodev,discard wait,formattable,forceencrypt=/dev/block/bootdevice/by-name/metadata
/dev/block/bootdevice/by-name/boot     /boot      emmc   defaults defaults
/dev/block/bootdevice/by-name/vbmeta   /vbmeta    emmc   defaults defaults
/dev/block/bootdevice/by-name/vbmeta_system /vbmeta_system emmc defaults defaults
/dev/block/bootdevice/by-name/dtbo     /dtbo      emmc   defaults defaults
/dev/block/bootdevice/by-name/misc     /misc      emmc   defaults defaults
/dev/block/bootdevice/by-name/persist  /persist   ext4   defaults defaults
