# Recovery FSTAB for Nubia NX669J-S (Advanced)
/dev/block/bootdevice/by-name/system         /system         ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/system_ext     /system_ext     ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/vendor         /vendor         ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/vendor_dlkm    /vendor_dlkm    ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/product        /product        ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/system_dlkm    /system_dlkm    ext4    ro,barrier=1 wait,logical
/dev/block/bootdevice/by-name/metadata       /metadata       ext4    defaults wait,formattable,quota,reserved_size=128M
/dev/block/bootdevice/by-name/super          /super          ext4    defaults wait,resize,logical
/dev/block/bootdevice/by-name/userdata       /data           f2fs    rw,nosuid,nodev,discard wait,formattable,forceencrypt=/dev/block/bootdevice/by-name/metadata,quota
/dev/block/bootdevice/by-name/boot           /boot           emmc    defaults defaults
/dev/block/bootdevice/by-name/dtbo           /dtbo           emmc    defaults defaults
/dev/block/bootdevice/by-name/misc           /misc           emmc    defaults defaults
/dev/block/bootdevice/by-name/persist        /persist        ext4    defaults defaults
/dev/block/bootdevice/by-name/cache          /cache          ext4    defaults defaults
/dev/block/bootdevice/by-name/bootloader     /bootloader     emmc    defaults defaults
/dev/block/bootdevice/by-name/fastboot       /fastboot       emmc    defaults defaults
/dev/block/bootdevice/by-name/usb-otg        /usb-otg        auto    rw,defaults
/dev/block/bootdevice/by-name/sdcard         /external_sd    vfat    rw,umask=0000
/dev/block/bootdevice/by-name/crypto         /crypto         auto    defaults
/dev/block/bootdevice/by-name/recovery       /recovery       emmc    defaults defaults
/dev/block/bootdevice/by-name/vbmeta         /vbmeta         emmc    defaults defaults
/dev/block/bootdevice/by-name/vbmeta_system  /vbmeta_system  emmc    defaults defaults
